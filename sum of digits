#include<stdio.h>
int main()
{
      int num,rem,result=0;
      printf("Enter a number ");
      scanf("%d",&num);
      while(num>0)
      {
              rem=num%10;
              result=result+rem;
              num=num/10;
       }
       printf("The sum of all digits of given number = %d",result);
       return 0;
  }     
       
       
       
