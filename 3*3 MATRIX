#include<stdio.h>
int main()
{
int i,j,a[3][3],sum=0;
for(i=0;i<3;i++)
{
for(j=0;j<3;j++)
{
printf("Enter element at %d%d position :",i+1,j+1);
scanf("%d",&a[i][j]);
}
}
for(i=0;i<3;i++)
{
for(j=0;j<3;j++)
{
printf("%d",a[i][j]);
}
printf("\n");
}
for(i=0;i<3;i++)
{
sum=sum+a[i][i];
}
printf("\n The sum of diagonal elements of a matrix = %d",sum);
return 0;
}
